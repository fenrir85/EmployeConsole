@model NwlEmployeeConsole.Models.SearchModel
@{
    ViewData["Title"] = "Home Page";
    var x = Model.WordKey;

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>

        <table cellpadding="0" cellspacing="0">
            <tr>
                <th colspan="2" align="center">Search Keyword:</th>
            </tr>
            <tr>

                <td>
                    @Model.WordKey
                </td>

            </tr>

        </table>

        <table id="datatable" class="display" style="width:100%">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Position</th>
                    <th>Office</th>
                    <th>Age</th>
                    <th>Start date</th>
                    <th>Salary</th>
                </tr>
            </thead>
            <tfoot>
                <tr>
                    <th>Name</th>
                    <th>Position</th>
                    <th>Office</th>
                    <th>Age</th>
                    <th>Start date</th>
                    <th>Salary</th>
                </tr>
            </tfoot>
        </table>


    </div>
</body>
</html>


@section scripts {
    <script type="text/javascript">
        $(document).ready(function () {
            $('#datatable').DataTable();
        });
    </script>
}